{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "strictNullChecks": true,
    "removeComments": false,
    "sourceMap": true,
    "declaration": true,
    "noEmitOnError": true,
    "rootDir": ".",
    "outDir": "",
    "lib": ["es5", "dom", "es2015.core", "es2015.promise", "es2015.collection", "es2015.iterable"]
  },
  "include": [
    "ts_src/**/*.ts"
  ]
}
